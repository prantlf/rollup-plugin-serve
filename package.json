{
  "name": "@prantlf/rollup-plugin-serve",
  "version": "3.2.0",
  "description": "Rollup plugin to serve the bundle and other files in the project directory.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/index.mjs",
    "default": "./dist/index.cjs"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -cw",
    "fix": "prettier --write .",
    "lint": "biome lint",
    "prepare": "npm run fix && npm run build",
    "test": "npm run lint && cd test && rollup -cw || cd .."
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "serve",
    "dev-server",
    "static"
  ],
  "license": "MIT",
  "author": "Ferdinand Prantl <prantlf@gmail.com>",
  "contributors": [
    "Thomas Ghysels <info@thomasg.be>"
  ],
  "homepage": "https://github.com/prantlf/rollup-plugin-serve",
  "bugs": {
    "url": "https://github.com/prantlf/rollup-plugin-serve/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/rollup-plugin-serve"
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "type": "module",
  "dependencies": {
    "compression": "^1.8.1",
    "connect-history-api-fallback": "^2.0.0",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "killable": "^1.0.1",
    "mime": "^4",
    "opener": "1",
    "serve-index": "^1.9.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.3",
    "prettier": "^3",
    "rollup": "4"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "semi": false,
    "printWidth": 120
  }
}